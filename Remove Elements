class Solution {
    public int removeElement(int[] nums, int val) {
        int j = 0;
        int l = nums.length;
        for (int i = 0; i < l; i++) {
            int n = nums[i];
            if (n != val) {
                nums[j] = n;
                j++;
              }
        }
        return j;
    }
}